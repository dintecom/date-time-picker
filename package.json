{
  "name": "date-time-picker-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "test": "ng test",
    "build-lib": "ng build picker",
    "test-lib": "ng test picker",
    "build-css": "sass projects/picker/src/sass/picker.scss dist/picker/assets/style/picker.min.css --no-source-map --style=compressed",
    "artifacts": "node copy-artifacts.js",
    "package": "ng build picker && npm run build-css && npm run artifacts",
    "publish": "cd dist/picker && npm publish",
    "npm-update": "ncu",
    "npm-upgrade": "ncu --upgrade && npm install"
  },
  "private": true,
  "engines": {
    "npm": ">=6.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@angular/animations": "~12.2.3",
    "@angular/cdk": "^12.2.3",
    "@angular/common": "~12.2.3",
    "@angular/compiler": "~12.2.3",
    "@angular/core": "~12.2.3",
    "@angular/forms": "~12.2.3",
    "@angular/platform-browser": "~12.2.3",
    "@angular/platform-browser-dynamic": "~12.2.3",
    "@angular/router": "~12.2.3",
    "moment-timezone": "^0.5.33",
    "rxjs": "~7.3.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.3",
    "@angular-eslint/builder": "~12.3.1",
    "@angular-eslint/eslint-plugin": "~12.3.1",
    "@angular-eslint/eslint-plugin-template": "~12.3.1",
    "@angular-eslint/schematics": "~12.3.1",
    "@angular-eslint/template-parser": "~12.3.1",
    "@angular/cli": "~12.2.3",
    "@angular/compiler-cli": "~12.2.3",
    "@angular/language-service": "^12.2.3",
    "@types/jasmine": "~3.8.2",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "eslint": "7.32.0",
    "eslint-plugin-deprecation": "1.2.1",
    "jasmine-core": "~3.9.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^12.2.1",
